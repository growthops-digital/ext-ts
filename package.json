{
	"name": "@growthops/ext-ts",
	"version": "3.1.1",
	"description": "TypeScript utility library for the GO Digital modern stack",
	"main": "dist/commonjs/index.js",
	"types": "dist/commonjs/index.d.ts",
	"module": "dist/es/index.js",
	"jsnext:main": "dist/es/index.js",
	"files": [
		"dist"
	],
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"test": "jest --passWithNoTests --coverage --runInBand",
		"lint": "eslint ."
	},
	"xo": {
		"extends": "xo-react",
		"envs": [
			"browser",
			"node",
			"jest"
		],
		"rules": {
			"unicorn/no-array-callback-reference": 0,
			"newline-before-return": 2
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/growthops-digital/ext-ts.git"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-datocms": "^3.0.12",
		"remeda": "0.0.32"
	},
	"devDependencies": {
		"@growthops/eslint-config": "^1.1.5",
		"@jest/globals": "^27.5.1",
		"@types/jest": "^27.4.1",
		"@types/react": "^17.0.43",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^8.12.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"jest": "^27.5.1",
		"react": "^18.0.0",
		"react-datocms": "^3.0.12",
		"remeda": "0.0.32",
		"ts-jest": "^27.1.4",
		"typescript": "^4.6.3"
	}
}
